{
  "name": "react-jsx-mock-mono",
  "version": "1.0.0",
  "description": "Mock components through JSX",
  "main": "index.js",
  "author": "Eamon Hyland <ehyland90@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "preconstruct dev",
    "test": "yarn workspace with-react-scripts test",
    "lint": "manypkg check && prettier --check . && tsc --noEmit",
    "format": "prettier --write .",
    "build": "preconstruct build",
    "build:watch": "preconstruct watch",
    "postinstall": "preconstruct dev",
    "add:change": "changeset add",
    "ci:version": "changeset version && yarn",
    "ci:release": "yarn build && changeset publish"
  },
  "workspaces": {
    "packages": [
      "packages/*",
      "examples/*"
    ],
    "nohoist": [
      "with-react-scripts/react-scripts"
    ]
  },
  "preconstruct": {
    "packages": [
      "packages/*"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@changesets/changelog-github": "^0.4.1",
    "@changesets/cli": "^2.17.0",
    "@manypkg/cli": "^0.18.0",
    "@preconstruct/cli": "^2.1.1",
    "@types/jest": "^27.0.1",
    "jest": "^27.1.0",
    "prettier": "^2.3.2",
    "typescript": "^4.4.2"
  }
}
